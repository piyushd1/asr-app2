luding a robust noise reduction library (noisereduce) and superior \nVoice Activity Detection (VAD) to dramatically improve transcription quality. \nIt uses the Indic Whisper model for ASR and Sarvam-Translate for translation.\n\"\"\"\n\nfrom __future__ import annotations\nimport torch\nimport torchaudio\nimport gradio as gr\nimport gc\nimport warnings\nimport numpy as np\nfrom typing import List, Tuple, Dict, Any, Optional\nimport time\nimport os\nimport shutil\nimport traceback\nimport logging\nimport sys\nimport json\nfrom pathlib import Path\nfrom datetime import datetime\nfrom contextlib import contextmanager\nimport threading\nimport psutil\n\n# Suppress warnings for cleaner output\nwarnings.filterwarnings(\"ignore\", category=UserWarning)\nwarnings.filterwarnings(\"ignore\", category=FutureWarning)\nos.environ['TOKENIZERS_PARALLELISM'] = 'false'\n\n# ==================== LOGGING SETUP ====================\nprint(\"🚀 ADVANCED ASR SYSTEM - STARTING...\")\nprint(\"🔧 Upgraded with Indic Whisper, NoiseReduce, Silero VAD, and Sarvam Translate\")\nprint(\"=\" * 60)\n\n# Simple logging setup\nlogging.basicConfig(\n    level=logging.INFO,\n    format='%(asctime)s - %(levelname)s - %(message)s',\n    handlers=[logging.StreamHandler(sys.stdout)]\n)\nlogger = logging.getLogger(__name__)\n\n# ==================== GLOBAL STATE WITH THREAD SAFETY ====================\nclass ModelState:\n    \"\"\"Thread-safe model state management\"\"\"\n    \n    def __init__(self):\n        self._lock = threading.Lock()\n        self._asr_model = None\n        self._asr_processor = None # Added for Whisper model\n        self._translation_model = None\n        self._translation_tokenizer = None\n        self._vad_model = None\n        self._preproc_models_loaded = False\n        self._model_loaded = False\n        self._translation_available = False\n        self._loading_status = \"Not started\"\n        self._errors = []\n        self._device = \"unknown\"\n    \n    @property\n    def asr_model(self):\n        with self._lock:\n            return self._asr_model\n    \n    @asr_model.setter\n    def asr_model(self, value):\n        with self._lock:\n            self._asr_model = value\n\n    @property\n    def asr_processor(self):\n        with self._lock:\n            return self._asr_processor\n    \n    @asr_processor.setter\n    def asr_processor(self, value):\n        with self._lock:\n            self._asr_processor = value\n\n    @property\n    def translation_model(self):\n        with self._lock:\n            return self._translation_model\n\n    @translation_model.setter\n    def translation_model(self, value):\n        with self._lock:\n            self._translation_model = value\n\n    @property\n    def translation_tokenizer(self):\n        with self._lock:\n            return self._translation_tokenizer\n\n    @translation_tokenizer.setter\n    def translation_tokenizer(self, value):\n        with self._lock:\n            self._translation_tokenizer = value\n    \n    @property\n    def vad_model(self):\n        with self._lock:\n            return self._vad_model\n            \n    @vad_model.setter\n    def vad_model(self, value):\n        with self._lock:\n            self._vad_model = value\n\n    @property\n    def preproc_models_loaded(self):\n        with self._lock:\n            return self._preproc_models_loaded\n\n    @preproc_models_loaded.setter\n    def preproc_models_loaded(self, value):\n        with self._lock:\n            self._preproc_models_loaded = value\n\n    @property\n    def model_loaded(self):\n        with self._lock:\n            return self._model_loaded\n    \n    @model_loaded.setter\n    def model_loaded(self, value):\n        with self._lock:\n            self._model_loaded = value\n    \n    @property\n    def translation_available(self):\n        with self._lock:\n            return self._translation_available\n    \n    @translation_available.setter\n    def translation_available(self, value):\n        with self._lock:\n            self._translation_available = value\n    \n    @property\n    def loading_status(self):\n        with self._lock:\n            return self._loading_status\n    \n    @loading_status.setter\n    def loading_status(self, value):\n        with self._lock:\n            self._loading_status = value\n    \n    @property\n    def device(self):\n        with self._lock:\n            return self._device\n    \n    @device.setter\n    def device(self, value):\n        with self._lock:\n            self._device = value\n    \n    def add_error(self, error):\n        with self._lock:\n            self._errors.append(error)\n    \n    def get_errors(self):\n        with self._lock:\n            return self._errors.copy()\n\n# Global model state\nmodel_state = ModelState()\n\n# ==================== ENVIRONMENT SETUP ====================\nprint(\"🔧 Setting up environment...\")\n\n# Environment detection\nis_kaggle = os.path.exists('/content') or 'KAGGLE_KERNEL_RUN_TYPE' in os.environ\nis_colab = 'COLAB_GPU' in os.environ\nenv_type = \"Kaggle\" if is_kaggle else \"Colab\" if is_colab else \"Local\"\n\nprint(f\"🌍 Environment: {env_type}\")\n\n# Set environment variables\nenv_vars = {\n    'HF_HUB_DISABLE_SYMLINKS_WARNING': '1',\n    'TOKENIZERS_PARALLELISM': 'false',\n    'GRADIO_ANALYTICS_ENABLED': '0',\n    'TRANSFORMERS_CACHE': '/content/cache' if (is_kaggle or is_colab) else './cache',\n}\n\nfor key, value in env_vars.items():\n    os.environ[key] = value\n    print(f\"    {key}={value}\")\n\nos.environ['HF_HOME'] = os.environ['TRANSFORMERS_CACHE']\n\n# ==================== DEVICE DETECTION ====================\nprint(\"🖥️ Detecting device...\")\n\ntry:\n    if torch.cuda.is_available():\n        device = 'cuda'\n        device_name = torch.cuda.get_device_properties(0).name\n        print(f\"✅ GPU found: {device_name}\")\n    else:\n        device = 'cpu'\n        device_name = f\"CPU ({os.cpu_count()} cores)\"\n        print(f\"✅ Using CPU with {os.cpu_count()} cores\")\nexcept Exception as e:\n    print(f\"⚠️ Device detection issue: {e}\")\n    device = 'cpu'\n    device_name = \"CPU (fallback)\"\n\nmodel_state.device = device\n\n# ==================== LANGUAGE CONFIGURATION ====================\n# ASR model language codes\nLANGUAGE_NAME_TO_ASR_CODE = {\n    \"Assamese\": \"as\", \"Bengali\": \"bn\", \"Bodo\": \"br\", \"Dogri\": \"doi\",\n    \"Gujarati\": \"gu\", \"Hindi\": \"hi\", \"Kannada\": \"kn\", \"Kashmiri\": \"ks\",\n    \"Konkani\": \"kok\", \"Maithili\": \"mai\", \"Malayalam\": \"ml\", \"Manipuri\": \"mni\",\n    \"Marathi\": \"mr\", \"Nepali\": \"ne\", \"Odia\": \"or\", \"Punjabi\": \"pa\",\n    \"Sanskrit\": \"sa\", \"Santali\": \"sat\", \"Sindhi\": \"sd\", \"Tamil\": \"ta\",\n    \"Telugu\": \"te\", \"Urdu\": \"ur\"\n}\n\n# Translation model language codes (for Sarvam Translate)\nASR_CODE_TO_TRANSLATE_CODE = {\n    \"as\": \"as_IN\", \"bn\": \"bn_IN\", \"gu\": \"gu_IN\", \"hi\": \"hi_IN\", \n    \"kn\": \"kn_IN\", \"ml\": \"ml_IN\", \"mr\": \"mr_IN\", \"or\": \"or_IN\", \n    \"pa\": \"pa_IN\", \"ta\": \"ta_IN\", \"te\": \"te_IN\", \"ur\": \"ur_IN\"\n    # Other languages will default to Hindi\n}\n\n\n# ==================== UTILITY FUNCTIONS ====================\ndef safe_function(func):\n    \"\"\"\n    Decorator to catch exceptions and return a format compatible with Gradio's four outputs.\n    \"\"\"\n    def wrapper(*args, **kwargs):\n        func_name = func.__name__\n        try:\n            print(f\"🔄 {func_name}...\")\n            result = func(*args, **kwargs)\n            print(f\"✅ {func_name} completed\")\n            return result\n        except Exception as e:\n            error_msg = f\"❌ {func_name} failed: {str(e)}\"\n            print(error_msg)\n            logger.error(f\"{func_name} error: {e}\\n{traceback.format_exc()}\")\n            model_state.add_error(error_msg)\n            # FIX: Return 4 values to match Gradio's expected output on error\n            return \"\", \"\", \"\", error_msg\n    return wrapper\n\ndef format_status_display():\n    status_parts = [\n        f\"Device: {device_name}\",\n        f\"Pre-proc: {'✅' if model_state.preproc_models_loaded else '❌'}\",\n        f\"ASR Model: {'✅' if model_state.model_loaded else '❌'}\",\n        f\"Translation: {'✅' if model_state.translation_available else '❌'}\",\n    ]\n    return \" | \".join(status_parts)\n\n# ==================== MODEL LOADING ====================\n@safe_function\ndef load_preproc_models():\n    \"\"\"Load audio pre-processing models (VAD only).\"\"\"\n    print(\"🔊 Loading pre-processing models (VAD)...\")\n    model_state.loading_status = \"🔄 Loading VAD model...\"\n    try:\n        # Load Silero VAD\n        vad_model, utils = torch.hub.load(repo_or_dir='snakers4/silero-vad', model='silero_vad', force_reload=True)\n        model_state.vad_model = (vad_model, utils)\n        \n        model_state.preproc_models_loaded = True\n        print(\"✅ Pre-processing models loaded.\")\n        return True\n    except Exception as e:\n        model_state.add_error(f\"Pre-proc model load failed: {e}\")\n        print(f\"❌ Failed to load pre-processing models: {e}\")\n        return False\n\n@safe_function\ndef load_asr_model():\n    \"\"\"Load Indic Whisper ASR model and processor.\"\"\"\n    print(\"🚀 Loading ASR model (Indic Whisper)...\")\n    model_state.loading_status = \"🔄 Loading ASR model...\"\n    from transformers import AutoProcessor, AutoModelForSpeechSeq2Seq\n    \n    model_name = \"parthiv11/indic_whisper_hi_multi_gpu\"\n    \n    # Load the processor which includes the feature extractor and tokenizer\n    processor = AutoProcessor.from_pretrained(model_name)\n    \n    # Load the model\n    model = AutoModelForSpeechSeq2Seq.from_pretrained(\n        model_name,\n        torch_dtype=torch.float16 if device == 'cuda' else torch.float32,\n        low_cpu_mem_usage=True,\n        use_safetensors=True\n    ).to(device).eval()\n    \n    model_state.asr_processor = processor\n    model_state.asr_model = model\n    model_state.model_loaded = True\n    model_state.loading_status = \"✅ ASR model ready (Indic Whisper)!\"\n    return True\n\n@safe_function\ndef load_translation_model():\n    \"\"\"Load the Sarvam Translate model from Hugging Face.\"\"\"\n    print(\"🌍 Loading translation model (Sarvam Translate)...\")\n    model_state.loading_status = \"🔄 Loading translation model...\"\n    from transformers import AutoTokenizer, AutoModelForSeq2SeqLM\n\n    # Using the powerful Sarvam Translate model\n    model_name = \"sarvamai/sarvam-translate\"\n    \n    tokenizer = AutoTokenizer.from_pretrained(model_name)\n    model = AutoModelForSeq2SeqLM.from_pretrained(model_name).to(device).eval()\n    \n    model_state.translation_tokenizer = tokenizer\n    model_state.translation_model = model\n    model_state.translation_available = True\n    print(\"✅ Translation model ready (Sarvam Translate)\")\n    return True\n\ndef load_models_synchronized():\n    \"\"\"Load all models synchronously\"\"\"\n    print(\"\\n🤖 STARTING SYNCHRONIZED MODEL LOADING\")\n    load_preproc_models()\n    load_asr_model()\n    load_translation_model()\n    model_state.loading_status = \"🎉 All systems ready!\"\n    print(\"🎉 System ready for transcription!\")\n\n# ==================== AUDIO PRE-PROCESSING (UPGRADED) ====================\n@safe_function\ndef preprocess_audio(audio_path: str):\n    \"\"\"\n    Applies a full pre-processing pipeline to the audio file.\n    1. Denoise the audio using noisereduce.\n    2. Apply VAD to find speech segments.\n    3. Concatenate and normalize only the speech segments.\n    Returns a tuple (waveform, sample_rate) or raises an exception on failure.\n    \"\"\"\n    if not model_state.preproc_models_loaded:\n        raise RuntimeError(\"Pre-processing models are not loaded.\")\n    \n    import noisereduce as nr\n\n    # 1. Load audio\n    waveform, sr = torchaudio.load(audio_path)\n    if sr != 16000:\n        waveform = torchaudio.functional.resample(waveform, sr, 16000)\n        sr = 16000\n    if waveform.shape[0] > 1:\n        waveform = waveform.mean(dim=0, keepdim=True)\n\n    # 2. Denoise using noisereduce\n    print(\"    🔊 Denoising audio...\")\n    waveform_np = waveform.squeeze().numpy()\n    denoised_np = nr.reduce_noise(y=waveform_np, sr=sr)\n    denoised_waveform = torch.from_numpy(denoised_np).unsqueeze(0)\n\n    # 3. VAD - Get speech timestamps\n    print(\"    🗣️ Detecting voice activity...\")\n    vad_model, utils = model_state.vad_model\n    (get_speech_timestamps, _, _, *_) = utils\n    speech_timestamps = get_speech_timestamps(waveform, vad_model, sampling_rate=16000)\n\n    if not speech_timestamps:\n        print(\"    ⚠️ No speech detected in the audio.\")\n        return None, 16000\n\n    # 4. Collect speech chunks from the *denoised* audio\n    print(f\"    Collecting speech from {len(speech_timestamps)} segments...\")\n    speech_chunks = [denoised_waveform[0, ts['start']:ts['end']] for ts in speech_timestamps]\n    collected_speech = torch.cat(speech_chunks).unsqueeze(0)\n\n    # 5. Normalize audio\n    print(\"    🎚️ Normalizing audio...\")\n    target_dbfs = -20.0\n    rms = collected_speech.norm(p=2) / (collected_speech.numel() ** 0.5)\n    if rms > 0:\n        normalized_speech = collected_speech * ((10 ** (target_dbfs / 20)) / rms)\n    else:\n        normalized_speech = collected_speech\n\n    return normalized_speech, 16000\n\n# ==================== TRANSLATION (UPGRADED) ====================\n@safe_function\ndef translate_text(text: str, src_lang_code: str) -> str:\n    \"\"\"Translate text using the Sarvam model, handling long text.\"\"\"\n    if not text or not model_state.translation_available:\n        return text or \"\"\n\n    lines = text.strip().split('\\n')\n    translated_lines = []\n\n    for line in lines:\n        if not line.strip():\n            continue\n        \n        speaker_part, text_to_translate = \"\", line\n        if \": \" in line:\n            parts = line.split(\": \", 1)\n            speaker_part, text_to_translate = parts[0] + \": \", parts[1]\n\n        if not text_to_translate.strip():\n            translated_lines.append(speaker_part)\n            continue\n            \n        # Set the source language for the tokenizer\n        model_state.translation_tokenizer.src_lang = src_lang_code\n        \n        # Tokenize\n        inputs = model_state.translation_tokenizer(text_to_translate, return_tensors=\"pt\", padding=True, truncation=True).to(device)\n        \n        # Generate\n        with torch.no_grad():\n            generated_tokens = model_state.translation_model.generate(\n                **inputs,\n                forced_bos_token_id=model_state.translation_tokenizer.lang_code_to_id[\"en_XX\"]\n            )\n        \n        # Decode\n        translated_text = model_state.translation_tokenizer.batch_decode(generated_tokens, skip_special_tokens=True)[0]\n        translated_lines.append(speaker_part + translated_text)\n\n    return \"\\n\".join(translated_lines)\n\n# ==================== MAIN TRANSCRIPTION (UPGRADED) ====================\n@safe_function\ndef transcribe_audio(audio_path: str, language_name: str, chunk_length: int = 30, # Whisper works best with 30s chunks\n                     enable_diarization: bool = True, enable_translation: bool = True):\n    if not audio_path:\n        return \"Please upload an audio file\", \"\", \"\", \"\"\n    if not model_state.model_loaded:\n        return \"⏳ Model is still loading...\", \"\", \"\", model_state.loading_status\n\n    start_time = time.time()\n    \n    # 1. Advanced Pre-processing\n    waveform, sample_rate = preprocess_audio(audio_path)\n    if waveform is None:\n        return \"No speech detected in the audio.\", \"\", \"\", \"Failed\"\n        \n    duration = waveform.shape[-1] / sample_rate\n    print(f\"⏱️ Cleaned audio duration: {duration:.1f}s\")\n\n    # 2. Diarization (on the cleaned audio)\n    speaker_segments = [(0, duration, \"Speaker_1\")]\n    if enable_diarization:\n        speaker_segments = simple_speaker_diarization(waveform, sample_rate)\n\n    # 3. Chunking and Transcription\n    asr_lang_code = LANGUAGE_NAME_TO_ASR_CODE.get(language_name, \"hi\")\n    chunk_samples = chunk_length * sample_rate\n    chunks = [waveform[..., i:min(i + chunk_samples, waveform.shape[-1])] for i in range(0, waveform.shape[-1], chunk_samples)]\n    \n    transcriptions = [process_audio_chunk(chunk.squeeze().numpy(), asr_lang_code, idx + 1) for idx, chunk in enumerate(chunks)]\n    \n    full_transcription_text = [f\"{speaker_segments[i % len(speaker_segments)][2]}: {text}\" for i, text in enumerate(transcriptions)]\n    full_transcription = \"\\n\".join(full_transcription_text)\n    \n    # 4. Translation\n    english_translation = \"\"\n    if enable_translation and model_state.translation_available:\n        translate_lang_code = ASR_CODE_TO_TRANSLATE_CODE.get(asr_lang_code, \"hi_IN\")\n        english_translation = translate_text(full_transcription, src_lang_code=translate_lang_code)\n\n    # 5. Final Info\n    diarization_info = f\"Speakers Found: {len(set(s[2] for s in speaker_segments))}\"\n    status = f\"✅ Completed! Processed {duration:.1f}s audio in {time.time() - start_time:.1f}s\"\n    \n    return full_transcription, english_translation, diarization_info, status\n\n# Placeholder functions from original code that are still needed\n@safe_function\ndef simple_speaker_diarization(waveform: torch.Tensor, sample_rate: int = 16000, \n                               min_speech_duration: float = 0.5) -> List[Tuple[float, float, str]]:\n    frame_length, hop_length = int(0.025 * sample_rate), int(0.010 * sample_rate)\n    energy = [torch.sqrt(torch.mean(waveform[0, i:i + frame_length] ** 2)).item() for i in range(0, waveform.shape[1] - frame_length, hop_length)]\n    threshold = np.percentile(energy, 30)\n    is_speech = np.array(energy) > threshold\n    segments, start_idx, speaker = [], None, \"Speaker_1\"\n    for i, speech in enumerate(is_speech):\n        if speech and start_idx is None: start_idx = i\n        elif not speech and start_idx is not None:\n            if (i - start_idx) * 0.010 > min_speech_duration:\n                segments.append((start_idx * 0.010, i * 0.010, speaker))\n                speaker = \"Speaker_2\" if speaker == \"Speaker_1\" else \"Speaker_1\"\n            start_idx = None\n    if start_idx is not None: segments.append((start_idx * 0.010, len(is_speech) * 0.010, speaker))\n    return segments if segments else [(0, waveform.shape[1]/sample_rate, \"Speaker_1\")]\n\n@safe_function\ndef process_audio_chunk(chunk_np: np.ndarray, lang_code: str, chunk_idx: int) -> str:\n    \"\"\"Process an audio chunk using the Whisper model.\"\"\"\n    if not model_state.model_loaded: return \"[Model not ready]\"\n    \n    processor = model_state.asr_processor\n    model = model_state.asr_model\n    \n    # Use the processor to prepare the audio for the model\n    input_features = processor(chunk_np, sampling_rate=16000, return_tensors=\"pt\").input_features\n    input_features = input_features.to(device, dtype=torch.float16 if device == \"cuda\" else torch.float32)\n\n    # Set the language for transcription\n    forced_decoder_ids = processor.get_decoder_prompt_ids(language=lang_code, task=\"transcribe\")\n\n    # Generate the token IDs\n    with torch.no_grad():\n        predicted_ids = model.generate(input_features, forced_decoder_ids=forced_decoder_ids)\n    \n    # Decode the token IDs to text\n    transcription = processor.batch_decode(predicted_ids, skip_special_tokens=True)[0]\n    return transcription.strip()\n\n\n# ==================== GRADIO INTERFACE (Simplified for clarity) ====================\ndef create_interface():\n    print(\"🎨 Creating Gradio interface...\")\n    with gr.Blocks(theme=gr.themes.Soft()) as demo:\n        gr.Markdown(\"# 🎙️ Advanced ASR System\\n**Powered by Indic Whisper, NoiseReduce, Silero VAD, and Sarvam Translate**\")\n        \n        with gr.Row():\n            status_display = gr.Textbox(label=\"📊 System Status\", interactive=False)\n            refresh_btn = gr.Button(\"🔄 Refresh\")\n\n        with gr.Row():\n            with gr.Column():\n                audio_input = gr.Audio(label=\"📁 Upload Audio File\", type=\"filepath\")\n                language = gr.Dropdown(label=\"🌐 Language\", choices=list(LANGUAGE_NAME_TO_ASR_CODE.keys()), value=\"Hindi\")\n                with gr.Row():\n                    enable_diarization = gr.Checkbox(label=\"👥 Speaker Diarization\", value=True)\n                    enable_translation = gr.Checkbox(label=\"🌍 Translate to English\", value=True)\n                process_btn = gr.Button(\"🚀 Process Audio\", variant=\"primary\")\n            \n            with gr.Column():\n                processing_status = gr.Textbox(label=\"📊 Processing Status\", interactive=False)\n                speaker_info = gr.Textbox(label=\"👥 Speaker Information\", interactive=False)\n                with gr.Tabs():\n                    with gr.Tab(\"📝 Transcription\"):\n                        transcription = gr.Textbox(label=\"Original Text\", lines=8, show_copy_button=True)\n                    with gr.Tab(\"🌍 Translation\"):\n                        translation = gr.Textbox(label=\"English Translation\", lines=8, show_copy_button=True)\n\n        # Event Handlers\n        refresh_btn.click(lambda: format_status_display(), outputs=[status_display])\n        demo.load(lambda: format_status_display(), outputs=[status_display])\n        \n        process_btn.click(\n            fn=transcribe_audio,\n            inputs=[audio_input, language, enable_diarization, enable_translation],\n            outputs=[transcription, translation, speaker_info, processing_status]\n        )\n    return demo\n\n# ==================== MAIN EXECUTION ====================\ndef main():\n    \"\"\"Main execution with synchronized model loading\"\"\"\n    try:\n        load_models_synchronized()\n        demo = create_interface()\n        print(\"\\n🚀 Launching interface...\")\n        demo.queue().launch(share=True, debug=False)\n    except Exception as e:\n        print(f\"\\n💥 Critical error in main execution: {e}\")\n        print(traceback.format_exc())\n\nif __name__ == \"__main__\":\n    # ==============================================================================\n    # IMPORTANT: RUN THIS CELL IN YOUR NOTEBOOK *BEFORE* RUNNING THE APPLICATION\n    # ==============================================================================\n    # !pip install -q \"transformers==4.38.2\" \"accelerate\" \"sentencepiece\" \"silero-vad\" \"noisereduce\"\n    # ==============================================================================\n    main()","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-08-07T09:47:30.616031Z","iopub.execute_input":"2025-08-07T09:47:30.616848Z","iopub.status.idle":"2025-08-07T09:47:40.949964Z","shell.execute_reply.started":"2025-08-07T09:47:30.616816Z","shell.execute_reply":"2025-08-07T09:47:40.949390Z"}},"outputs":[{"name":"stdout","text":"🚀 ADVANCED ASR SYSTEM - STARTING...\n🔧 Upgraded with Indic Whisper, NoiseReduce, Silero VAD, and Sarvam Translate\n============================================================\n🔧 Setting up environment...\n🌍 Environment: Kaggle\n    HF_HUB_DISABLE_SYMLINKS_WARNING=1\n    TOKENIZERS_PARALLELISM=false\n    GRADIO_ANALYTICS_ENABLED=0\n    TRANSFORMERS_CACHE=/content/cache\n🖥️ Detecting device...\n✅ GPU found: Tesla T4\n\n🤖 STARTING SYNCHRONIZED MODEL LOADING\n🔄 load_preproc_models...\n🔊 Loading pre-processing models (VAD)...\n","output_type":"stream"},{"name":"stderr","text":"Downloading: \"https://github.com/snakers4/silero-vad/zipball/master\" to /root/.cache/torch/hub/master.zip\n","output_type":"stream"},{"name":"stdout","text":"✅ Pre-processing models loaded.\n✅ load_preproc_models completed\n🔄